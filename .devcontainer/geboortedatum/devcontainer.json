{
    "name": "Webformulier met JSX oefening",
    "image": "mcr.microsoft.com/devcontainers/universal:2",
    "hostRequirements": {
      "cpus": 4
    },
    "workspaceFolder": "/webformulier",
    "workspaceMount": "source=${localWorkspaceFolder}/oefeningen/geboortedatum,target=/webformulier,type=bind,consistency=cached",
    "portsAttributes": {
      "3000": {
        "label": "Web applicatie",
        "onAutoForward": "openPreview"
      }
    },
    "forwardPorts": [3000],
    "waitFor": "onCreateCommand",
    "postCreateCommand": "yarn install",
    "postAttachCommand": "yarn start",
    "customizations": {
      "codespaces": {
        "openFiles": ["src/index.js"]
      }
    }
  }
  